body
  padding: 0
  margin: 0
  font: 14px "Lucida Grande", Helvetica, Arial, sans-serif
  background-color: #000
  
a
  color: #00B7FF

#time
  width: 300px
  display: inline

#video-primary
  height:100%

#video-clone
  height 50%
  -webkit-transform rotate(180deg)
  -moz-transform rotate(180deg)

#controls-wrapper
  background-color: rgba(0, 0, 0, 0.90)
  border-radius: 4px
  color: #fff

#btn-duplicate
  z-index: 20
  top: 0 
  right: 0
  position: absolute
