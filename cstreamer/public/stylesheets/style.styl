body
  padding: 0
  margin: 0
  font: 14px "Lucida Grande", Helvetica, Arial, sans-serif
  background-color: #000
  
a
  color: #00B7FF

#time
  width: 300px
  display: inline

#hidden-video
  display none

.video-container
  width 100%
  height 1000%
  display inline-block
  position relative

#controls-wrapper
  background-color: rgba(0, 0, 0, 0.90)
  border-radius: 4px
  color: #fff
  text-align center
  z-index: 1000
  top: 50%
  position: absolute
  left: 50%
  margin-left: -250px
  margin-top -15px

#btn-duplicate
  z-index: 20
  top: 0 
  right: 0
  position: absolute
